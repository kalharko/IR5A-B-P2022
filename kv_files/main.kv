#:kivy 2.1.0



<Token>:
    canvas:
        Rectangle:
            source: self.texture
            pos: self.pos
            size: self.size




<Map>:
    
    canvas:
        Rectangle:
            pos: root.pos
            size: root.size
            source: self.texture

    


<GameSpace>:
    map: map
    pingManager: pingManager

    Map:
        id: map
        center: root.center
        #size: root.size

    PingManager :
        id : pingManager



<MainWidget>:
    
    gameSpace: gameSpace

    GameSpace:
        id: gameSpace


    









