#:kivy 2.1.0



<Token>:
    canvas:
        Rectangle:
            source: self.texture
            pos: self.pos
            size: self.size




<Map>:
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source: self.texture



<MainWidget>:
    map: map

    Map:
        id: map
        center: root.center
        #size: root.size

